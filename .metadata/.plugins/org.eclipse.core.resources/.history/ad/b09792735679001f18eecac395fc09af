void automatic_run(){
	switch(status)
	{
	case (INIT):
			break;
	case (GREEN_RED):
			break;
	case (YELLOW_RED):
			break;
	case (RED_GREEN):
			break;
	case (RED_YELLOW):
			break;
	default:
		break;
	}
}
